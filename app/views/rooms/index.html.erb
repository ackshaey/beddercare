<div id="newRoom">
	<fieldset>
		<legend>
			Start a new party room
		</legend>
			<%= form_for(@new_room) do |f| %>
			Party Name : <%= f.text_field :name %>
			Public? : <%= f.check_box :public %>
			<%= f.submit %>
			<% end %>
	</fieldset>
</div>
<div id="roomList">
	<% @rooms.each do |room| %>
		Name : <%= room.name %>
		Created : <%= room.created_at %>
		Session ID: <%= room.sessionId %>
		<a href=party/<%= room.id %>>Join</a>
		<hr/>	
	<% end %>
</div>